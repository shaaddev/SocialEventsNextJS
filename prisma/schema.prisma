// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model posts {
  id Int @id @default(autoincrement())
  title String 
  caption String
  location String
  event_date String
  created_at DateTime @default(now())
}
